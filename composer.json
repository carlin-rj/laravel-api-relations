{
    "name": "carlin/laravel-api-relations",
    "description": "Eloquent-like API relationships for Laravel with composite key support and N+1 prevention",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "api",
        "relations",
        "composite-key",
        "eager-loading"
    ],
    "authors": [
        {
            "name": "carlin",
            "email": "rjwangnixingfu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Carlin\\LaravelApiRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carlin\\LaravelApiRelations\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
